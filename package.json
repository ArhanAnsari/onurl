{
  "name": "onurl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality": "npm run format && npm run lint",
    "codequality:fix": "npm run format:fix && npm run lint:fix",
    "db:migrate": "dotenv -e .env.development -- npx prisma db push",
    "dev": "next dev",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "postinstall": "prisma generate",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "postinstall:about": "echo 'https://www.prisma.io/docs/guides/other/troubleshooting-orm/help-articles/vercel-caching-issue'",
    "prepare": "husky install",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "file-saver": "^2.0.5",
    "go-try": "^2.0.2",
    "nanoid": "^5.0.4",
    "next": "^14.0.3",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-share": "^5.0.3",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.6",
    "@types/node": "^20.10.3",
    "@types/qrcode.react": "^1.0.4",
    "@types/react": "^18.2.41",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-packagejson": "^2.4.7",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  }
}
