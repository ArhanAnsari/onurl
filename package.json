{
  "name": "onurl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality": "npm run lint && npm run format",
    "codequality:fix": "npm run lint:fix && npm run format:fix",
    "db:migrate": "dotenv -e .env.development -- npx prisma db push",
    "dev": "next dev",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "postinstall": "prisma generate",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "postinstall:about": "echo 'https://www.prisma.io/docs/guides/other/troubleshooting-orm/help-articles/vercel-caching-issue'",
    "prepare": "husky install",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "go-try": "^2.0.2",
    "nanoid": "^5.0.2",
    "next": "^14.0.2",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-share": "^4.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.6",
    "@types/node": "^20.8.10",
    "@types/qrcode.react": "^1.0.4",
    "@types/react": "^18.2.35",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
