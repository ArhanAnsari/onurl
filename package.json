{
  "name": "onurl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "codequality": "npm run lint && npm run format",
    "codequality:fix": "npm run lint:fix && npm run format:fix",
    "typecheck": "tsc",
    "prepare": "husky install",
    "db:migrate": "dotenv -e .env.development -- npx prisma db push",
    "about:postinstall": "echo 'https://www.prisma.io/docs/guides/other/troubleshooting-orm/help-articles/vercel-caching-issue'",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "go-try": "^2.0.2",
    "nanoid": "^4.0.2",
    "next": "^13.4.12",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-share": "^4.4.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/node": "^20.4.5",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^18.2.17",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
